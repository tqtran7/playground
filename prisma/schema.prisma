generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Action {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  action  String
  key String
  value Json
  created DateTime @default(now())
}
